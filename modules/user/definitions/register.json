{
    "version": "2",
	"label": "Register",
	"image":"user/panel_register.png",
    "item": [
		{
			"type":"text",
			"name":"heading",
			"label":"Heading",
			"default": "register here"
		},
		{
			"type":"text",
			"name":"register_fullname",
			"label":"Full name label",
			"default": "full name"
		},
		{
			"type":"text",
			"name":"register_email",
			"label":"Email label",
			"default": "email"
		},
		{
			"type":"text",
			"name":"register_submit",
			"label":"Submit label",
			"default": "register"
		},
		{
			"type":"link",
			"name":"redirect_link",
			"label":"Success URL",
			"help": "Redirect to this URL after successful login"
		},
		{
			"type":"textarea",
			"name":"loggedin_text",
			"label":"Logged in message",
			"lines":"2",
			"default": "You are currently logged in. Please log out to register a new account."
		},
		{
			"type":"subtitle",
			"label":"Username"
		},
		{
			"type":"select",
			"label":"Ask for username",
			"name":"show_username",
			"values":{
				"yes":"Yes",
				"no":"No"
			},
			"default":"yes"
		},
		{
			"type":"text",
			"name":"register_username",
			"label":"Username label",
			"default": "username"
		},
		{
			"type":"subtitle",
			"label":"Password"
		},
		{
			"type":"select",
			"label":"Ask for password",
			"name":"show_password",
			"values":{
				"yes":"Yes",
				"no":"No"
			},
			"default":"yes"
		},
		{
			"type":"text",
			"name":"register_password",
			"label":"Password label",
			"default": "new password"
		},
		{
			"type":"text",
			"name":"register_password2",
			"label":"Password x2 label",
			"default": "new password again"
		}
	]
}
	
